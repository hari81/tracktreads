{
  "expo": {
    "name": "Undercarriage Inspection",
    "description": "This app is provided to customers inspecting Undercarriage machines",
    "slug": "undercarriage-inspection",
    "privacy": "public",
    "sdkVersion": "29.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.1",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tracktreads.undercarriage",
      "buildNumber": "1.0.8",
      "infoPlist": {
        "NSCameraUsageDescription": "Undercarriage Inspection uses the camera to take photos of components and equipments"
      }
    }
  }
}
