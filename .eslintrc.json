{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "indent": [2, "tab"],
        "no-tabs": 0,
        "global-require": 0,
        "no-console": 0,
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/jsx-indent": 0,
        "react/prop-types": 0,
        "import/no-named-as-default-member": 0,
        "import/no-named-as-default": 0,
        "no-underscore-dangle": 0,
        "camelcase": [2, {"properties": "always"}],
        "globals": { "_": false }
    }
}